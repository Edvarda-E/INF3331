"(#.*)": comment
"(\bdef\s+)(?:\w+\s*)(?:\(.*?\):)" : function
"(\".*?\"|\'.*?\')": strings
"(\bimport\b\s*)(?:\w+\s*)|((\bfrom\b )(?:\w+\s*))" : import
"(\bTrue\b|\bNone\b|\bFalse\b)" : special
"(\@\w+)(?:\((?:(?:\s*\w+\s*)+[\=,]?)*\))?": decorators
"(\btry:)|(\bexcept )(?:\w+)(?: as \w+)?(?::)": try_except
"(\bfor )(?:\w+(?:\s*,\s*\w+)*?)( in )(?:\w+(?:\.\w+)*)(?:(?:\(.*?\)))*:": for_loops
"(\bwhile\b\s*)(?:\(\s*)?(?:\w+\s*|\w+\s*(?:==|!=|<>|>|<|>=|<=)\s*\w+)(?:\s*\))?(?::)": while_loops
"(\bif\b|\belif\b|\belse\b)": if_else