"#.*?$": comment
"(\bdef\h+)(?:\w+\h*)(?:\(.*?\):)" : function
"\".*?\"|\'.*?\'": strings
"(\bimport\b\h*)(?:\w+\h*)|((\bfrom\b )(?:\w+\h*))" : import
"\bTrue\b|\bNone\b|\bFalse\b" : special
"(\@\w+)(?:\((?:(?:\h*\w+\h*)+[\=,]?)*\))?": decorators
"(\btry:\n)|(\bexcept )(?:\w+)(?: as \w+)?(?::)": try_except
"(\bfor )(?:\w+(?:\h*,\h*\w+)*?)( in )(?:\w+(?:\.\w+)*)(?:(?:\(.*?\)))*:": for_loops
"(\bwhile\b\h*)(?:\(\h*)?(?:\w+\h*|\w+\h*(?:==|!=|<>|>|<|>=|<=)\h*\w+)(?:\h*\))?(?::)": while_loops

